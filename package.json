    "@emotion/react": "11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.9.0",
    "@types/react": "^18.3.3",
    "@types/react-router-dom": "^5.3.3",
    "ajv": "^8.16.0",
    "axios": "1.7.2",
    "chakra-ui-autocomplete": "^1.4.5",
    "cross-fetch": "^4.0.0",
    "downshift": "9.0.6",
    "file-loader": "^6.2.0",
    "framer-motion": "^11.2.13",
    "gapi-script": "^1.2.0",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "history": "^5.3.0",
    "hugeicons-react": "^0.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-confetti": "^6.1.0",
    "react-countdown-circle-timer": "^3.2.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.2.2",
    "react-hook-form": "^7.52.1",
    "react-icons": "^5.2.1",
    "react-loader-spinner": "^6.1.6",
    "react-papaparse": "^4.4.0",
    "react-router-dom": "^6.24.1",
    "react-scripts": "^5.0.1",
    "react-searchable-dropdown": "^2.0.3",
    "react-select": "^5.8.0",
    "recharts": "^2.12.7",
    "styled-components": "^6.1.11",
    "styled-system": "^5.1.5",
    "terser-webpack-plugin": "^5.3.10",
    "type-fest": "^4.21.0",
    "typescript": "^5.5.3",
    "zod": "^3.23.8"
  },
  "scripts": {
    "start": "HTTPS=true REACT_APP_API_MOCKING=false craco start",
    "clean": "rm -rf build/* ",
    "build": "craco build",
    "test": "jest --forceExit --maxWorkers=2 --silent --testTimeout=180000",
    "lint": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .prettierignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "check-format": "npm run prettier -- --list-different",
    "prebuild": "npm run generate:all",
    "validate": "npm-run-all --parallel check-types && lint-staged",
    "prepare": "husky install",
    "generate:clean": "find . -type f -name '*.generated.tsx' -delete",
    "generate:all": "npm run generate:clean && graphql-codegen --config codegen.yml",
    "clear-jest-cache": "jest --clearCache",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@chakra-ui/storybook-addon": "^5.1.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.6.0",
    "@faker-js/faker": "^8.4.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-graphql-files-modules": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/builder-webpack5": "^8.1.11",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^8.1.11",
    "@testing-library/dom": "^10.3.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/history": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.6",
    "@types/react-dom": "^18.3.0",
    "@types/react-facebook-login": "^4.1.11",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "babel-plugin-lodash": "^3.3.4",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^9.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-webpack-plugin": "^4.2.0",
    "factory.ts": "^1.4.1",
    "globals": "^15.8.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "mini-css-extract-plugin": "^2.9.0",
    "prettier": "^3.3.2",
    "sonarqube-scanner": "^4.0.1",
    "ts-jest": "^29.1.5",
    "tsc-files": "^1.1.4",
    "webpack": "^5.92.1",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
